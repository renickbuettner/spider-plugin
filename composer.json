{
    "name": "renick/spider-plugin",
    "type": "october-plugin",
    "description": "OctoberCMS plugin for crawling your local site, finding not found errors, warm up all caches and resizer images.",
    "require": {
        "composer/installers": "~1.0",
        "renick/spatie-crawler": "dev-main"
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    },
    "repositories": {
        "renick/spatie-crawler": {
            "type": "git",
            "url": "git@github.com:renickbuettner/spatie-crawler.git"
        }
    }
}
